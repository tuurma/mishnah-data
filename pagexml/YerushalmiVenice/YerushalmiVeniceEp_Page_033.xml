        <Baseline points="476,1274 2629,1259"/>
        <TextEquiv>
          <Unicode>עת לעשות לה׳. הלל הזקן היה אומר בשעה דמכנשין בדר. ובשעה</Unicode>
        </TextEquiv>
      </TextLine>

      
      
      <TextLine id="eSc_line_d8d3c92d" custom="structure {type:Main;}">
        <Coords points="481,1374 476,1309 2629,1314 2634,1364 2624,1445 476,1435"/>
        <Baseline points="481,1374 2634,1364"/>
