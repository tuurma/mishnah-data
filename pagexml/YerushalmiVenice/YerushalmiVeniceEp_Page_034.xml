        <Baseline points="1139,5614 3286,5604"/>
        <TextEquiv>
          <Unicode>וכן צוה משה את יהושע. וכן עשה יהושע לא הסיר דבר מכל אשר צוה אותו</Unicode>
        </TextEquiv>
      </TextLine>

      
      
      <TextLine id="eSc_line_cb3b2dd3" custom="structure {type:Main;}">
