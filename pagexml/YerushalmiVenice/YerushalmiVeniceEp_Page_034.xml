        <Baseline points="1134,5288 3291,5298"/>
        <TextEquiv>
          <Unicode>חור למטה יהודה עשה את כל אשר צוה אותו משה אין כתיב כאן אלא אשר</Unicode>
        </TextEquiv>
      </TextLine>

      
      
      <TextLine id="eSc_line_99d9ccb8" custom="structure {type:Main;}">
