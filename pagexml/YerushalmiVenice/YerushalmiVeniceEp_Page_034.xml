        <Baseline points="1139,5810 3281,5825"/>
        <TextEquiv>
          <Unicode>שמע מפי רבו הסכימה דעתו כמה שנאמר למשה בסיני. רבי יוחנן בשם רבו</Unicode>
        </TextEquiv>
      </TextLine>

      
      
      <TextLine id="eSc_line_eaf19b25" custom="structure {type:Main;}">
