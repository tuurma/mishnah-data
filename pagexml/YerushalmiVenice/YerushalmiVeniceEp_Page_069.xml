        <Baseline points="2795,1811 4962,1821"/>
        <TextEquiv>
          <Unicode>לרבי חלבו רבי חלבו שאל לרבי אימי רבי אימי שאל לרבי אלעזר רבי לעזר</Unicode>
        </TextEquiv>
      </TextLine>

      
      
      <TextLine id="eSc_line_d6838a8c" custom="structure {type:Main;}">
