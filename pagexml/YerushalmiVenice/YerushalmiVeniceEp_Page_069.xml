        <Baseline points="2800,963 4952,978"/>
        <TextEquiv>
          <Unicode>דהוא מה הוא מנכי: רבי לעזר אמר דברים בגב. רבי חנינא אמר דברי׳ בגב.</Unicode>
        </TextEquiv>
      </TextLine>

      
      
      <TextLine id="eSc_line_146039b1" custom="structure {type:Main;}">
