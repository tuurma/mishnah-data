        <Baseline points="2815,1831 4988,1851"/>
        <TextEquiv>
          <Unicode>מקודש. קם רבי יוסה עם רבי יודה בן פזי אמר ליה את שמעת מן אבוך הדא</Unicode>
        </TextEquiv>
      </TextLine>

      
      
      <TextLine id="eSc_line_b946594c" custom="structure {type:Main;}">
