        <Baseline points="2820,1194 4988,1219"/>
        <TextEquiv>
          <Unicode>אחר עיבורו יום אחד יכול יהא מעובר תלמוד לומר אותם אתם אלה הם</Unicode>
        </TextEquiv>
      </TextLine>

      
      
      <TextLine id="eSc_line_3bc7a894" custom="structure {type:Main;}">
        <Coords points="2815,1299 2815,1249 4982,1274 4988,1324 4972,1394 2805,1359"/>
        <Baseline points="2815,1299 4988,1324"/>
        <TextEquiv>
          <Unicode>מועדי לפני זמנו אין אלה מועדי לפני זמנו עשרים ותשעה יום לאחר עיבורו</Unicode>
        </TextEquiv>
      </TextLine>

      
      
      <TextLine id="eSc_line_afb4cd9c" custom="structure {type:Main;}">
