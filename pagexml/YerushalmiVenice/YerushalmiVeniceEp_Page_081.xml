        <Baseline points="2820,1510 4993,1535"/>
        <TextEquiv>
          <Unicode>הגיעו תלמוד לומר בני ישראל מועדי. עשה את המועדות שיעשו אותן כל</Unicode>
        </TextEquiv>
      </TextLine>

      
      
      <TextLine id="eSc_line_bb61182a" custom="structure {type:Main;}">
