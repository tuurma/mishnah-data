        </TextEquiv>
      </TextLine>

      
      
      <TextLine id="eSc_line_18e05346" custom="structure {type:Main;}">
        <Coords points="2820,2669 2820,2619 4982,2619 4988,2679 4972,2749 2810,2729"/>
        <Baseline points="2820,2669 4988,2679"/>
