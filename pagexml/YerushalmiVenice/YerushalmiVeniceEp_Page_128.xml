        <Baseline points="3512,1389 5675,1399"/>
        <TextEquiv>
          <Unicode>רבי בא בר ממל שמותר לעשות כן בתחילה לא לשעבר. מיי כדון אם יש בו</Unicode>
        </TextEquiv>
      </TextLine>

      
      
      <TextLine id="eSc_line_a72c7a9e" custom="structure {type:Main;}">
        <Coords points="3507,1495 3507,1445 5675,1455 5680,1505 5665,1575 3492,1555"/>
        <Baseline points="3507,1495 5680,1505"/>
