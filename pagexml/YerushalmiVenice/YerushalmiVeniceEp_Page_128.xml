        <Baseline points="3537,4802 5710,4791"/>
        <TextEquiv>
          <Unicode>מתירין הוא. אית דתני לה בשם רבי מאיר. אית דלא תני לה בשם רבי מאיר</Unicode>
        </TextEquiv>
      </TextLine>

      
      
      <TextLine id="eSc_line_d3d2ec2a" custom="structure {type:Main;}">
