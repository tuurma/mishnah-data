        <Baseline points="2825,792 5003,807"/>
        <TextEquiv>
          <Unicode>נותן רשות לשנים שעירבו ואין שנים שעירבו נותנין רשות לאחד שלא עירב</Unicode>
        </TextEquiv>
      </TextLine>

      
      
      <TextLine id="eSc_line_600fc2a2" custom="structure {type:Main;}">
