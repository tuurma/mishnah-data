        <Baseline points="2815,3657 4972,3657"/>
        <TextEquiv>
          <Unicode>שמאי. דבית שמאי אומרים אין מבטלין רשות משתחשך. ובית הלל אומרים</Unicode>
        </TextEquiv>
      </TextLine>

      
      
      <TextLine id="eSc_line_17762313" custom="structure {type:Main;}">
        <Coords points="2815,3768 2815,3718 4988,3718 4988,3768 4982,3843 2810,3828"/>
        <Baseline points="2815,3768 4988,3768"/>
