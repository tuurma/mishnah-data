        <Baseline points="2820,3331 4982,3346"/>
        <TextEquiv>
          <Unicode>רשות משתחשך. אמר רבי יוסה בי רבי בון מכיון שמת אין ביטול רשות גדול</Unicode>
        </TextEquiv>
      </TextLine>

      
      
      <TextLine id="eSc_line_df1e681e" custom="structure {type:Main;}">
