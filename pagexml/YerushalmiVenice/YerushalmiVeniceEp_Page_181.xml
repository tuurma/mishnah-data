        <Baseline points="2815,2704 4977,2714"/>
        <TextEquiv>
          <Unicode>כיני מתניתא בית הלל אומרים משתחשך. דל כן היא מתניתא מקולי בית שמאי</Unicode>
        </TextEquiv>
      </TextLine>

      
      
      <TextLine id="eSc_line_f5e01302" custom="structure {type:Main;}">
