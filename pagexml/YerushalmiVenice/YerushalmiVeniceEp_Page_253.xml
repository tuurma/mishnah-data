        </TextEquiv>
      </TextLine>

      
      
      <TextLine id="eSc_line_d5affced" custom="structure {type:Main;}">
        <Coords points="2754,3331 2749,3286 4937,3281 4942,3331 4937,3407 2749,3397"/>
        <Baseline points="2754,3331 4942,3331"/>
