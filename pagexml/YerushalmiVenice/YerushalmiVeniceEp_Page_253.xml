        </TextEquiv>
      </TextLine>

      
      
      <TextLine id="eSc_line_182f3b24" custom="structure {type:Main;}">
        <Coords points="2754,3106 2754,3045 4937,3065 4942,3116 4927,3191 2744,3176"/>
        <Baseline points="2754,3106 4942,3116"/>
        <TextEquiv>
          <Unicode>אין פוחתין מעשרה מלכיות כנגד עשרה קילוסין שאמר דוד הללויה הללו אל</Unicode>
        </TextEquiv>
      </TextLine>

      
      
      <TextLine id="eSc_line_fe6a604b" custom="structure {type:Main;}">
        <Coords points="2749,3226 2749,3181 4942,3186 4942,3226 4942,3296 2749,3296"/>
        <Baseline points="2749,3226 4942,3226"/>
        <TextEquiv>
