        <Baseline points="2795,1971 4977,1971"/>
        <TextEquiv>
          <Unicode>הושעיה ובלבד משש שעות ולמעלן. אמר רבי יוסי תרתיהון לקולא. אבל</Unicode>
        </TextEquiv>
      </TextLine>

      
      
      <TextLine id="eSc_line_edd423d9" custom="structure {type:Main;}">
