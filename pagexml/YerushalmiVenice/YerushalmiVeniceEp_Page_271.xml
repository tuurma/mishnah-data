        <Baseline points="2790,2082 4982,2082"/>
        <TextEquiv>
          <Unicode> סעודת תשעה באב משש שעות ולמטן אפילו עולה על שולחנו כסעודת שלמה</Unicode>
        </TextEquiv>
      </TextLine>

      
      
      <TextLine id="eSc_line_df2dd720" custom="structure {type:Main;}">
