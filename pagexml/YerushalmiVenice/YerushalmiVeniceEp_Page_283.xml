        <Baseline points="2795,2343 5003,2343"/>
        <TextEquiv>
          <Unicode>תורגמן שהוא עומד לפני חכם אינו רשאי לא לשנו׳ ולא לכנו׳ ולא להוסיף אלא</Unicode>
        </TextEquiv>
      </TextLine>

      
      
      <TextLine id="eSc_line_f1dca8be" custom="structure {type:Main;}">
