      <TextLine id="eSc_line_80bbee65" >
        <Coords points="3462,1766 3452,1721 5630,1711 5640,1756 5630,1836 3457,1831"/>
        <Baseline points="3462,1766 5640,1756"/>
        <TextEquiv>
          <Unicode>מת פלוני נהרג פלוני משיאין את אשתו. מצאו כתוב בשטר מת פלוני נחרג</Unicode>
        </TextEquiv>
      </TextLine>

      
      
      <TextLine id="eSc_line_3f8d2e7e" >
        <Coords points="3462,1861 3462,1816 5640,1826 5645,1871 5630,1941 3452,1931"/>
        <Baseline points="3462,1861 5645,1871"/>
