        <Baseline points="3487,5900 5645,5890"/>
        <TextEquiv>
          <Unicode>לבה נאה לא היה מגלהו. ואם היה שערה נאה לא היה סותרו. ו׳ היתה</Unicode>
        </TextEquiv>
      </TextLine>

      
      
      <TextLine id="eSc_line_239c59f5" >
