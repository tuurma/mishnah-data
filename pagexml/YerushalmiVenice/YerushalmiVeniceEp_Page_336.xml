        <Baseline points="3482,5163 5645,5153"/>
        <TextEquiv>
          <Unicode>עידי נפשות ואומ׳ לה בתי הרבה יין עושה. הרבה שחוק עושה. הרבה ילדות</Unicode>
        </TextEquiv>
      </TextLine>

      
      
      <TextLine id="eSc_line_6fdfb5ab" >
