        <Baseline points="2810,2649 4988,2649"/>
        <TextEquiv>
          <Unicode>את עבד. אמר ר׳ חמא בר חנינא משל למלך שהיה משיא את בנו ובא איפרכוס</Unicode>
        </TextEquiv>
      </TextLine>

      
      
      <TextLine id="eSc_line_576e568c" custom="structure {type:Main;}">
