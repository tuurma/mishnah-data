        <Baseline points="2795,1460 4982,1470"/>
        <TextEquiv>
          <Unicode>פרונא עבד חורן התיב רבי בא דהא תני גובה בגיטה ומטמנת כתובתה וממתנת</Unicode>
        </TextEquiv>
      </TextLine>

      
      
      <TextLine id="eSc_line_a6480025" custom="structure {type:Main;}">
