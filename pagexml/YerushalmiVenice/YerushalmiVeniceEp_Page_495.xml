    <TextRegion id="eSc_textblock_b47b41b9"  custom="structure {type:Main;}">
      <Coords points="5088,762 5128,822 5088,6518 4927,6804 4988,6844 4947,7019 3086,6959 2990,6899 3066,6723 2890,6718 2830,6598 3010,6558 2830,6477 2945,6377 2810,6377 2890,762"/>
      
      
      <TextLine id="eSc_line_78cc4935" custom="structure {type:Main;}">
        <Coords points="2895,812 2895,757 5068,772 5073,827 5058,903 2885,883"/>
        <Baseline points="2895,812 5073,827"/>
        <TextEquiv>
          <Unicode>תור או אימר או גדי לעבודה זרה ואמר ליה פייסיה עלי והוא אמר ליה למה ובי</Unicode>
        </TextEquiv>
      </TextLine>

      
      
      <TextLine id="eSc_line_239de802" custom="structure {type:Main;}">
        <Coords points="2900,923 2895,878 4340,863 4345,863 4345,863 4350,863 4436,888 4451,893 5063,893 5068,938 5053,1013 2890,983"/>
        <Baseline points="2900,923 5068,938"/>
        <TextEquiv>
          <Unicode>מה מועילה לך אינה אוכלת ואינה שותה ולא מטיבה ולא מריעה. והוא אמר</Unicode>
        </TextEquiv>
      </TextLine>

      
      
      <TextLine id="eSc_line_14f59a99" custom="structure {type:Main;}">
        <Coords points="2895,1028 2895,978 5053,988 5058,1048 5043,1118 2885,1088"/>
        <Baseline points="2895,1028 5058,1048"/>
        <TextEquiv>
          <Unicode>ליה היך נעביד והוא אמר ליה איזל ואייתי לי חד פינך דסולת ועשר ביעין עלוי</Unicode>
        </TextEquiv>
      </TextLine>

      
      
      <TextLine id="eSc_line_9d4fef89" custom="structure {type:Main;}">
        <Coords points="2895,1139 2895,1088 5053,1103 5058,1154 5043,1229 2885,1204"/>
        <Baseline points="2895,1139 5058,1154"/>
        <TextEquiv>
          <Unicode>ונא מפיים ליה עלך כיון דהוא אזל ליה הוה אכיל לון. אתא חד בר פחין</Unicode>
        </TextEquiv>
      </TextLine>

      
      
      <TextLine id="eSc_line_285789c8" custom="structure {type:Main;}">
        <Coords points="2900,1239 2900,1189 5058,1209 5063,1254 5048,1334 2890,1299"/>
        <Baseline points="2900,1239 5063,1254"/>
        <TextEquiv>
          <Unicode>ואמר ליה כן אמר ליה אם אינה מועילה כלום מפני מה את עביד הכא. אמר</Unicode>
        </TextEquiv>
      </TextLine>

      
      
      <TextLine id="eSc_line_15b714ea" custom="structure {type:Main;}">
        <Coords points="2900,1344 2900,1279 5058,1309 5063,1359 5048,1440 2890,1409"/>
        <Baseline points="2900,1344 5063,1359"/>
        <TextEquiv>
          <Unicode>ליה בנין חיי. מתיכין לרבי שמואל בר נחמן והא כתיב עד יום גלות הארץ. אמ׳</Unicode>
        </TextEquiv>
      </TextLine>

      
      
      <TextLine id="eSc_line_5d62d461" custom="structure {type:Main;}">
        <Coords points="2935,1460 2935,1409 5053,1415 5058,1470 5043,1545 2925,1520"/>
        <Baseline points="2935,1460 5058,1470"/>
        <TextEquiv>
          <Unicode>לון כיון שמת דוד ועמד שלמה והחליף את כל סנקליטין שלו חזר לקילקולו.</Unicode>
        </TextEquiv>
      </TextLine>

      
      
      <TextLine id="eSc_line_40b8327b" custom="structure {type:Main;}">
        <Coords points="2895,1560 2895,1510 5053,1525 5058,1575 5043,1650 2885,1620"/>
        <Baseline points="2895,1560 5058,1575"/>
        <TextEquiv>
          <Unicode>ונביא שעבר על דברי עצמו. כחבירו של מיכה. הדא היא דכתיב ונביא אחד</Unicode>
        </TextEquiv>
      </TextLine>

      
      
      <TextLine id="eSc_line_eba3db04" custom="structure {type:Main;}">
        <Coords points="2890,1665 2890,1615 5053,1630 5058,1680 5043,1756 2880,1726"/>
        <Baseline points="2890,1665 5058,1680"/>
        <TextEquiv>
          <Unicode>זקן יושב בביתל ויבא בנו ויספר לו את כל המעשה אשר עשה איש האלהי׳ וגו׳.</Unicode>
        </TextEquiv>
      </TextLine>

      
      
      <TextLine id="eSc_line_2037a6c0" custom="structure {type:Main;}">
        <Coords points="2890,1771 2890,1721 5048,1736 5053,1791 5038,1866 2880,1831"/>
        <Baseline points="2890,1771 5053,1791"/>
        <TextEquiv>
          <Unicode>ויאמר להם אביהם אי זה הדרך הלך וגו׳. ויאמ׳ אל בניו חבשו לי את החמור וגו׳.</Unicode>
        </TextEquiv>
      </TextLine>

      
      
      <TextLine id="eSc_line_73d33424" custom="structure {type:Main;}">
        <Coords points="2890,1881 2890,1831 4957,1846 4962,1891 4947,1951 2880,1941"/>
        <Baseline points="2890,1881 4962,1891"/>
        <TextEquiv>
          <Unicode>וילך אחרי איש האלהים וימצאהו יושב תחת האלה וגומ׳. ויאמר לך אתי</Unicode>
        </TextEquiv>
      </TextLine>

      
      
      <TextLine id="eSc_line_1e9c0531" custom="structure {type:Main;}">
        <Coords points="2885,1982 2885,1936 5063,1951 5063,1997 5048,2072 2875,2042"/>
        <Baseline points="2885,1982 5063,1997"/>
        <TextEquiv>
          <Unicode>הבית׳ ואכל לחם. ויאמר לא אוכל לשוב וגו׳ ע״ד ויאמר לו גם אני נביא כמוך</Unicode>
        </TextEquiv>
      </TextLine>

      
      
      <TextLine id="eSc_line_c95c4ec7" custom="structure {type:Main;}">
        <Coords points="2885,2097 2885,2052 5053,2057 5053,2097 5048,2182 2880,2162"/>
        <Baseline points="2885,2097 5053,2097"/>
        <TextEquiv>
          <Unicode>ומלאך דבר אלי בדבר י׳ לאמר וגו׳ ע״ד ויאכל לחם וישת מים כיחש לו. מה</Unicode>
        </TextEquiv>
      </TextLine>

      
      
      <TextLine id="eSc_line_24dbf218" custom="structure {type:Main;}">
        <Coords points="2885,2202 2885,2147 5053,2157 5058,2207 5043,2283 2875,2263"/>
        <Baseline points="2885,2202 5058,2207"/>
        <TextEquiv>
          <Unicode>הוא כיחש לו שיקר ביה. ויהי הם יושבים על השולחן ויהי דבר יי׳ אל הנביא אשר</Unicode>
        </TextEquiv>
      </TextLine>

      
      
      <TextLine id="eSc_line_3be4d739" custom="structure {type:Main;}">
        <Coords points="2890,2303 2890,2242 5053,2268 5058,2313 5043,2393 2880,2363"/>
        <Baseline points="2890,2303 5058,2313"/>
        <TextEquiv>
          <Unicode>חשיבו. אשר הושב אין כתיב כאן אלא אשר השיבו. והרי הדברים קל וחומ׳</Unicode>
        </TextEquiv>
      </TextLine>

      
      
      <TextLine id="eSc_line_36b331b5" custom="structure {type:Main;}">
        <Coords points="2885,2403 2885,2343 5048,2373 5053,2423 5038,2493 2875,2463"/>
        <Baseline points="2885,2403 5053,2423"/>
        <TextEquiv>
          <Unicode>ומה אם מי שהאכיל את חבירו לחם שקר זכה שנתייחד עליו הדיבר. המאכיל</Unicode>
        </TextEquiv>
      </TextLine>

      
      
      <TextLine id="eSc_line_c0a0d70e" custom="structure {type:Main;}">
        <Coords points="2885,2508 2885,2448 5048,2478 5053,2528 5038,2604 2875,2574"/>
        <Baseline points="2885,2508 5053,2528"/>
        <TextEquiv>
          <Unicode>את חבירו לחם אמת על אחת כמה וכמה. כתיב ואיש אחד מבני הנביאי׳ אמ׳ אל</Unicode>
        </TextEquiv>
      </TextLine>

      
      
      <TextLine id="eSc_line_dd45cd28" custom="structure {type:Main;}">
        <Coords points="2880,2619 2880,2569 5043,2584 5048,2639 5033,2709 2870,2679"/>
        <Baseline points="2880,2619 5048,2639"/>
        <TextEquiv>
          <Unicode>דעהו בדבר יי׳ וגו׳. ויאמר לו יען אשר לא שמעת בקול יי׳ הנך הולך מאתי והכך</Unicode>
        </TextEquiv>
      </TextLine>

      
      
      <TextLine id="eSc_line_4602a4bc" custom="structure {type:Main;}">
        <Coords points="2875,2724 2875,2674 5048,2694 5053,2744 5038,2814 2865,2784"/>
        <Baseline points="2875,2724 5053,2744"/>
        <TextEquiv>
          <Unicode>תאריה. וילך מאתו וימצאהו האריה ויכהו. וימצא איש אחר ויאמ׳ הכיני נא ויכהו</Unicode>
        </TextEquiv>
      </TextLine>

      
      
      <TextLine id="eSc_line_3b1f48c8" custom="structure {type:Main;}">
        <Coords points="2875,2830 2875,2784 5048,2799 5053,2850 5038,2925 2865,2890"/>
        <Baseline points="2875,2830 5053,2850"/>
        <TextEquiv>
          <Unicode>וגו׳. וילך הנביא ויעמד למלך על הדרך ויתחפש באפר על עיניו. ויהי המלך</Unicode>
        </TextEquiv>
      </TextLine>

      
      
      <TextLine id="eSc_line_985f3b01" custom="structure {type:Main;}">
        <Coords points="2860,2940 2860,2895 5048,2905 5048,2950 5033,3025 2845,3000"/>
        <Baseline points="2860,2940 5048,2950"/>
        <TextEquiv>
          <Unicode>עובר והוא צעק אל המלך וגו׳. ויהי עבדך עשה הנה והנה וגו. וימהר ויסר את</Unicode>
        </TextEquiv>
      </TextLine>

      
      
      <TextLine id="eSc_line_0a02d05a" custom="structure {type:Main;}">
        <Coords points="2870,3050 2870,3005 5043,3005 5048,3055 5033,3131 2860,3111"/>
        <Baseline points="2870,3050 5048,3055"/>
        <TextEquiv>
          <Unicode>האפר מעל עיניו וגו׳. ויאמר לו כה אמר יי׳ יען אשר שלחת את איש חרמי מיד</Unicode>
        </TextEquiv>
      </TextLine>

      
      
      <TextLine id="eSc_line_f1ecbaf6" custom="structure {type:Main;}">
        <Coords points="2870,3151 2870,3106 5048,3116 5053,3161 5038,3241 2860,3216"/>
        <Baseline points="2870,3151 5053,3161"/>
        <TextEquiv>
          <Unicode>והיתה נפשך תחת נפשו ועמך תחת עמו. וכתיב ויגש איש האלהים ויאמר כה</Unicode>
        </TextEquiv>
      </TextLine>

      
      
      <TextLine id="eSc_line_0864e56c" custom="structure {type:Main;}">
        <Coords points="2875,3266 2875,3221 5053,3221 5053,3266 5048,3341 2870,3331"/>
        <Baseline points="2875,3266 5053,3266"/>
        <TextEquiv>
          <Unicode>אמר יי׳. ולמה ויאמר ויאמ׳ שני פעמים. אלא באמירה הראשונה אמר לו אם בא</Unicode>
        </TextEquiv>
      </TextLine>

      
      
      <TextLine id="eSc_line_9b927020" custom="structure {type:Main;}">
        <Coords points="2865,3371 2865,3326 5043,3326 5043,3371 5038,3452 2860,3437"/>
        <Baseline points="2865,3371 5043,3371"/>
        <TextEquiv>
          <Unicode>בן הדד מלך ארם תחת ידך אל תחוםס עליו ואל תחמול עליו. ובאמירה השנייה</Unicode>
        </TextEquiv>
      </TextLine>

      
      
      <TextLine id="eSc_line_5c676bab" custom="structure {type:Main;}">
        <Coords points="2870,3467 2870,3422 5038,3427 5043,3477 5028,3557 2860,3532"/>
        <Baseline points="2870,3467 5043,3477"/>
        <TextEquiv>
          <Unicode>אמר לו יען אשר שלחת את איש חרמי וגו׳. כמה מצודות והרמים עשיתי לו עד</Unicode>
        </TextEquiv>
      </TextLine>

      
      
      <TextLine id="eSc_line_9d116d45" custom="structure {type:Main;}">
        <Coords points="2870,3577 2870,3527 5043,3537 5048,3587 5033,3652 2860,3637"/>
        <Baseline points="2870,3577 5048,3587"/>
        <TextEquiv>
          <Unicode>שמסרתיו תחת ידך ושלחתו והלך בשלו׳. לפיכך והיתה נפשך תחת נפשו ועמך</Unicode>
        </TextEquiv>
      </TextLine>

      
      
      <TextLine id="eSc_line_9afe7d37" custom="structure {type:Main;}">
        <Coords points="2865,3688 2865,3637 5038,3642 5043,3698 5028,3768 2855,3743"/>
        <Baseline points="2865,3688 5043,3698"/>
        <TextEquiv>
          <Unicode>תחת עמו. את מוצא בשעה שיצאו ישראל למלחמה לא מת מכולם אלא</Unicode>
        </TextEquiv>
      </TextLine>

      
      
      <TextLine id="eSc_line_3c22f3d5" custom="structure {type:Main;}">
        <Coords points="2865,3783 2865,3738 5038,3748 5043,3798 5028,3873 2855,3848"/>
        <Baseline points="2865,3783 5043,3798"/>
        <TextEquiv>
          <Unicode>אחאב מלך ישראל בלבד. והדא היא דכתיב ואיש משך בקשת לתומו ויך את</Unicode>
        </TextEquiv>
      </TextLine>

      
      
      <TextLine id="eSc_line_b0f6c459" custom="structure {type:Main;}">
        <Coords points="2865,3898 2865,3848 5038,3853 5043,3903 5028,3979 2855,3959"/>
        <Baseline points="2865,3898 5043,3903"/>
        <TextEquiv>
          <Unicode>מלך ישר׳ בין הדבקים ובין השיריין ויאמר לרבבו הפוך ידך והוציאיני מן המחנה</Unicode>
        </TextEquiv>
      </TextLine>

      
      
      <TextLine id="eSc_line_c45b94f8" custom="structure {type:Main;}">
        <Coords points="2870,3999 2870,3948 5033,3959 5038,4014 5023,4084 2860,4059"/>
        <Baseline points="2870,3999 5038,4014"/>
        <TextEquiv>
          <Unicode>כי החליתי. מה אני מקיים ועמך תחת עמו. רבי יוחנן בשם רבי שמעון בן יוחי</Unicode>
        </TextEquiv>
      </TextLine>

      
      
      <TextLine id="eSc_line_575b6918" custom="structure {type:Main;}">
        <Coords points="2935,4109 2940,4044 3010,4069 5033,4064 5043,4114 5028,4194 2925,4164"/>
        <Baseline points="2935,4109 5043,4114"/>
        <TextEquiv>
          <Unicode>אותה הטיפה שיצאת מאותו צדיק כיפרה על כל ישראל. הלכה ח׳</Unicode>
        </TextEquiv>
      </TextLine>

      
      
      <TextLine id="eSc_line_88dcd582" custom="structure {type:Main;}">
        <Coords points="2880,4214 2880,4149 4847,4174 4852,4219 4837,4280 2870,4275"/>
        <Baseline points="2880,4214 4852,4219"/>
        <TextEquiv>
          <Unicode>והמתנכא בשם עבודה זרה כו׳. אמר רבי יוסי בן חנינה הכל היה בכלל</Unicode>
        </TextEquiv>
      </TextLine>

      
      
      <TextLine id="eSc_line_4abb78c3" custom="structure {type:Main;}">
        <Coords points="2865,4325 2865,4280 5053,4280 5053,4325 5053,4395 2865,4410"/>
        <Baseline points="2865,4325 5053,4325"/>
        <TextEquiv>
          <Unicode> לא תענה ברעך עד שקר. יצא לידון בין באות בין במופת בין בעבודה זרה בין</Unicode>
        </TextEquiv>
      </TextLine>

      
      
      <TextLine id="eSc_line_234563d0" custom="structure {type:Main;}">
        <Coords points="2865,4435 2865,4390 5043,4390 5043,4435 5043,4516 2865,4516"/>
        <Baseline points="2865,4435 5043,4435"/>
        <TextEquiv>
          <Unicode> בשאר כל המצות. אלא שעבודה זרה בין שנתכוון לעקור את כל הגוף בין </Unicode>
        </TextEquiv>
      </TextLine>

      
      
      <TextLine id="eSc_line_d778b4f2" custom="structure {type:Main;}">
        <Coords points="2870,4531 2870,4490 5033,4490 5038,4541 5023,4621 3879,4596 3628,4601 3020,4616 3015,4616 3015,4616 2860,4586"/>
        <Baseline points="2870,4531 5038,4541"/>
        <TextEquiv>
          <Unicode>שלא נתכוון לעקור את כל הגוף דברי רבי שמעון פוטרין אותו. ודברי</Unicode>
        </TextEquiv>
      </TextLine>

      
      
      <TextLine id="eSc_line_14f57e77" custom="structure {type:Main;}">
        <Coords points="2870,4641 2870,4576 5033,4596 5038,4646 5023,4721 2860,4701"/>
        <Baseline points="2870,4641 5038,4646"/>
        <TextEquiv>
          <Unicode>חכמים סוקלין אותו. אבל בשאר כל המצות דברי חכמים סוקלין אותו ודברי ר׳</Unicode>
        </TextEquiv>
      </TextLine>

      
      
      <TextLine id="eSc_line_cd0875c9" custom="structure {type:Main;}">
        <Coords points="2860,4751 2850,4706 5023,4696 5033,4746 5023,4827 2855,4812"/>
        <Baseline points="2860,4751 5033,4746"/>
        <TextEquiv>
          <Unicode>שמעון יבין ליה פיונטייה. והנביא שנתנבה בתחילה אם נתן אות ומופת</Unicode>
        </TextEquiv>
      </TextLine>

      
      
      <TextLine id="eSc_line_c5181c64" custom="structure {type:Main;}">
        <Coords points="2865,4847 2865,4807 5028,4807 5033,4862 5023,4937 2885,4937 2880,4932 2880,4932 2875,4932 2875,4932 2875,4932 2870,4932 2870,4932 2870,4927 2865,4927 2840,4897"/>
        <Baseline points="2865,4847 5033,4862"/>
        <TextEquiv>
          <Unicode>שומעין לו ואם לאו אין שומעין לו. שני נביאים שנתנבאו כאחת. שני</Unicode>
        </TextEquiv>
      </TextLine>

      
      
      <TextLine id="eSc_line_82b2bc80" custom="structure {type:Main;}">
        <Coords points="2865,4962 2865,4917 5038,4922 5038,4962 5033,5042 2860,5022"/>
        <Baseline points="2865,4962 5038,4962"/>
        <TextEquiv>
          <Unicode>נביאים שנתנבאו בכרך אחד. רבי יצחק ורבי הושעיה חד אמר צריך</Unicode>
        </TextEquiv>
      </TextLine>

      
      
      <TextLine id="eSc_line_2c326efe" custom="structure {type:Main;}">
        <Coords points="2875,5072 2865,5022 5023,5017 5033,5062 5023,5148 2870,5138"/>
        <Baseline points="2875,5072 5033,5062"/>
        <TextEquiv>
          <Unicode>ליתן אות ומופת. וחרנה אמר אינו צריך ליתן אות ומופת. מתיב מאן דאמר</Unicode>
        </TextEquiv>
      </TextLine>

      
      
      <TextLine id="eSc_line_86e4d5db" custom="structure {type:Main;}">
        <Coords points="2975,5178 2965,5128 5023,5118 5033,5168 5023,5248 2970,5243"/>
        <Baseline points="2975,5178 5033,5168"/>
        <TextEquiv>
          <Unicode>צריך למאן דאמר אינו צריך והא כתיב ויאמר חזקיהו אל ישעיהו מה אות.</Unicode>
        </TextEquiv>
      </TextLine>

      
      
