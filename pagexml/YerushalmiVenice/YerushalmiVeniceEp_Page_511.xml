        <Baseline points="2890,1706 5073,1721"/>
        <TextEquiv>
          <Unicode>שומר שכר והשוכר. דתני לא מצינו בנושא שבר והשוכר שמשלם תשלומי</Unicode>
        </TextEquiv>
      </TextLine>

      
      
      <TextLine id="eSc_line_95d685f6" custom="structure {type:Main;}">
