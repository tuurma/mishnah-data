        <Baseline points="2880,2142 5083,2142"/>
        <TextEquiv>
          <Unicode>בשואל שמשלם תשלומי כפל בכל מקום. ותני עלה הרי זה ק״ל שאין עליו</Unicode>
        </TextEquiv>
      </TextLine>

      
      
      <TextLine id="eSc_line_c3bcf9a2" custom="structure {type:Main;}">
