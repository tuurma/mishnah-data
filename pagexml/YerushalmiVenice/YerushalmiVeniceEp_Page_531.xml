        <Baseline points="3020,2528 5198,2549"/>
        <TextEquiv>
          <Unicode>זה בעל התלמוד. ודל מבין יהקרנו זה בעל אגדה. לשנים שנכנסו לעיר</Unicode>
        </TextEquiv>
      </TextLine>

      
      
      <TextLine id="eSc_line_dda872f4" custom="structure {type:Main;}">
