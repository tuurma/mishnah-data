        <Baseline points="3020,2634 5193,2654"/>
        <TextEquiv>
          <Unicode>ביד זה עשתות של זהב. וביד זה פרוטרוט. זה שבידו עשתות של זהב</Unicode>
        </TextEquiv>
      </TextLine>

      
      
      <TextLine id="eSc_line_059a7b16" custom="structure {type:Main;}">
