        <Baseline points="3030,1038 5203,1058"/>
        <TextEquiv>
          <Unicode>להיסתבר במלכיות וראשיהן עומדין על גביהן הקדים ראשים לזקנים. תני</Unicode>
        </TextEquiv>
      </TextLine>

      
      
      <TextLine id="eSc_line_475d788c" custom="structure {type:Main;}">
