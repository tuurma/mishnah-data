        </TextEquiv>
      </TextLine>

      
      
      <TextLine id="eSc_line_2b8baef4" custom="structure {type:Main;}">
        <Coords points="3462,1856 3457,1816 5494,1806 5504,1861 5494,1926 3507,1936 3502,1936 3502,1936 3497,1936 3497,1936 3497,1936 3492,1936 3442,1906"/>
        <Baseline points="3462,1856 5504,1861"/>
        <TextEquiv>
          <Unicode>קדמאי אמר רבי יוסי בי רבי בון נפתח לגדול נפתח לקטן.</Unicode>
        </TextEquiv>
      </TextLine>

      
      
      <TextLine id="eSc_line_9ef650ea" custom="structure {type:Main;}">
        <Coords points="3316,1961 3311,1921 5389,1916 5394,1961 5389,2037 3311,2022"/>
        <Baseline points="3316,1961 5394,1961"/>
