        </TextEquiv>
      </TextLine>

      
      
      <TextLine id="eSc_line_bc97c9df" custom="structure {type:Main;}">
        <Coords points="3316,908 3306,863 5479,853 5489,898 5479,978 3311,973"/>
        <Baseline points="3316,908 5489,898"/>
