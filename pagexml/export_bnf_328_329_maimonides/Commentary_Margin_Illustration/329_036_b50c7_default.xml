      <TextRegion id="eSc_textblock_f82d0df1" custom="structure {type:Commentary;}">
         <Coords points="1886,1173 1901,2270 1174,2270 950,2182 601,2197 572,1217 616,1173"/>
         <TextLine id="eSc_line_8c77882b">
            <Coords points="589,1232 583,1172 1872,1183 1878,1221 1878,1282 1167,1320 1162,1320 583,1293"/>
            <Baseline points="593,1233 1880,1227"/>
            <TextEquiv>
               <Unicode>וכולן שפקעו פי' כל אדם שזכרנו שאם עלו לא ירדו</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_d62b129e">
            <Coords points="583,1320 578,1282 1867,1271 1872,1320 1872,1386 583,1403"/>
            <Baseline points="589,1321 1873,1326"/>
            <TextEquiv>
               <Unicode>אם פקעו מעל גבי המזבח לא יחזיר. וכן גחלת ש-</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_aa9c57ec">
            <Coords points="589,1408 578,1364 1861,1359 1872,1403 1861,1480 583,1474"/>
            <Baseline points="593,1409 1876,1407"/>
            <TextEquiv>
               <Unicode>פקעה גם כן לא יחזיר. ופי' פקעו עפו כמו שאנו</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_e083bd41">
            <Coords points="589,1491 583,1452 1861,1441 1872,1496 1856,1557 578,1551"/>
            <Baseline points="593,1494 1876,1499"/>
            <TextEquiv>
               <Unicode>רואין במוקדי האש שכל דבר שיש בתוכון לחות</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_e48b788b">
            <Coords points="589,1584 583,1546 1861,1540 1867,1584 1867,1656 589,1661"/>
            <Baseline points="593,1589 1869,1587"/>
            <TextEquiv>
               <Unicode>הוא עף מן האש כשמתחיל אותו לחות להתנועע</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_8f647f2e">
            <Coords points="594,1678 583,1639 1856,1612 1867,1672 1856,1760 589,1744"/>
            <Baseline points="596,1681 1869,1675"/>
            <TextEquiv>
               <Unicode>לצאת. ומה שאמ' קודם חצות ואחר חאות הלילה</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_fa334b19">
            <Coords points="589,1771 578,1733 1856,1716 1867,1766 1856,1848 583,1837"/>
            <Baseline points="593,1776 1869,1770"/>
            <TextEquiv>
               <Unicode>לפי שאמ' הכתו' היא העולה על מוקדה על המזבח</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_4efa39ae">
            <Coords points="594,1859 583,1821 1856,1815 1867,1854 1856,1942 589,1931"/>
            <Baseline points="596,1860 1869,1858"/>
            <TextEquiv>
               <Unicode>כל הלילה עד הבקר ואילו אמ' כל הלילה לבד היה</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_6a2d5199">
            <Coords points="594,1947 589,1914 1861,1909 1867,1947 1861,2024 589,2013"/>
            <Baseline points="596,1948 1869,1953"/>
            <TextEquiv>
               <Unicode>משמעו עד הבקר. ולפי שאמ' אחר כן עד הבקר</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_d384da3f">
            <Coords points="583,2024 578,1936 969,1931 974,1931 974,1931 980,1931 980,1931 980,1931 985,1931 1018,1953 1079,1991 1856,1997 1861,2035 1845,2112 572,2090"/>
            <Baseline points="585,2029 1865,2041"/>
            <TextEquiv>
               <Unicode>ופוא מה שאמ עד הכקר מן כקר להדלו ששלללל ועל</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_2a8b6bca">
            <Coords points="583,2123 578,2074 1861,2079 1867,2118 1861,2195 583,2212"/>
            <Baseline points="589,2124 1869,2122"/>
            <TextEquiv>
               <Unicode>כן אמרו עדחצות להרחיק אדם מן העבירה כמו ש-</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_dda0ef3a">
            <Coords points="1222,2212 1211,2173 1856,2151 1867,2206 1861,2278 1222,2283"/>
