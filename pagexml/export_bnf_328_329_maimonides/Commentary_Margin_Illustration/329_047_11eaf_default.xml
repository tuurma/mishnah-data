      <TextRegion id="eSc_textblock_8c4696dd" custom="structure {type:Commentary;}">
         <Coords points="3762,2655 3762,4082 3080,4111 2481,4053 2452,3050 2496,2655"/>
         <TextLine id="eSc_line_2094d569">
            <Coords points="2474,2714 2463,2670 3739,2665 3750,2708 3750,2763 2474,2790"/>
            <Baseline points="2480,2715 3753,2713"/>
            <TextEquiv>
               <Unicode>בעשה ולא תעשה ר"ל שאם עבר על מצות עשה</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_79f71d81">
            <Coords points="2485,2807 2474,2763 3733,2758 3744,2796 3739,2862 2485,2873"/>
            <Baseline points="2487,2810 3746,2800"/>
            <TextEquiv>
               <Unicode>ועל מצות לא תעשה שנ' השמר לך פן תעלה</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_8f322076">
            <Coords points="2485,2900 2474,2856 3728,2834 3739,2878 3739,2960 2485,2960"/>
            <Baseline points="2487,2901 3742,2884"/>
            <TextEquiv>
               <Unicode>עולותיך. וכבר בארנו בסוף מכות שהעקר בידינו</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_74854389">
            <Coords points="2491,2977 2480,2933 3722,2927 3733,2971 3728,3048 2491,3059"/>
            <Baseline points="2495,2981 3738,2972"/>
            <TextEquiv>
               <Unicode>כל מקום שנ' בו השמר פן ואל אינו אלא מצות לא</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_c1ff66f4">
            <Coords points="2463,3064 2452,3026 3722,3015 3733,3059 3728,3119 2463,3141"/>
            <Baseline points="2469,3065 3738,3063"/>
            <TextEquiv>
               <Unicode>תעשה. והעבירה על עשה הוא מה שנ' שמה תביאו</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_d3d481c5">
            <Coords points="2469,3152 2458,3108 3733,3102 3744,3146 3739,3206 2469,3212"/>
            <Baseline points="2473,3156 3746,3147"/>
            <TextEquiv>
               <Unicode>עולותיכם וגו' ראיה שלא יקריב אלא במקום שיבחר</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_2345a6ce">
            <Coords points="2474,3239 2474,3201 3744,3201 3744,3239 3744,3305 2474,3321"/>
            <Baseline points="2480,3244 3749,3242"/>
            <TextEquiv>
               <Unicode>ומעקרי תלמודינו לאו הבא מכלל עשה עשה. וכבר</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_a2f48f59">
            <Coords points="2496,3332 2485,3288 3733,3283 3744,3327 3739,3387 2496,3392"/>
            <Baseline points="2498,3335 3746,3329"/>
            <TextEquiv>
               <Unicode>בארנו זה ביבמות ומכות. וחייבין עליהן כרת לפ</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_ec42e1c1">
            <Coords points="2501,3414 2491,3360 3733,3354 3744,3403 3739,3475 2501,3491"/>
            <Baseline points="2506,3419 3746,3409"/>
            <TextEquiv>
               <Unicode>שהקדישן בשעת פיתר במות ואלי' הקריב אותן אז</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_da604acb">
            <Coords points="2469,3507 2458,3469 3733,3442 3744,3491 3739,3557 2469,3573"/>
            <Baseline points="2473,3510 3749,3493"/>
            <TextEquiv>
               <Unicode>בחוץ אילו חייב כלום ואינו חייב כרת אם הקריבן בחוץ.</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_b68606ed">
            <Coords points="2480,3589 2469,3551 3739,3535 3750,3579 3750,3666 2480,3666"/>
            <Baseline points="2484,3591 3753,3581"/>
            <TextEquiv>
               <Unicode>אלא על קרבן שאם הקריבו בשעת הקדישו בחוץ וּ'</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_d400868e">
            <Coords points="2496,3677 2485,3639 3739,3628 3750,3666 3744,3726 2496,3737"/>
            <Baseline points="2498,3682 3753,3668"/>
            <TextEquiv>
               <Unicode>חייב כרת למו שהיה במדבר בזמן שהיה אסור</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_2c17ffe4">
            <Coords points="2491,3759 2480,3721 3733,3693 3744,3748 3739,3830 2491,3830"/>
            <Baseline points="2495,3762 3746,3752"/>
            <TextEquiv>
               <Unicode>להקריב בחויץ. והכרק השלישי' שאמ' בו הרי אלו</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_400e16f6">
            <Coords points="2491,3852 2485,3808 3700,3808 3733,3776 3755,3841 3744,3929 2485,3918"/>
            <Baseline points="2495,3857 3757,3844"/>
            <TextEquiv>
               <Unicode>נעשה לפי שהקדיש בשעג איסור הבמת ולא</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_8c3fede5">
            <Coords points="2491,3940 2480,3901 3744,3885 3755,3934 3750,4005 2491,4016"/>
            <Baseline points="2495,3944 3760,3935"/>
            <TextEquiv>
               <Unicode>הקריב אז במקדש. והואיל והקריב חוץ למקדש</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_fdddd9db">
            <Coords points="2485,4027 2480,3978 3750,3978 3755,4016 3755,4093 2485,4093"/>
            <Baseline points="2487,4032 3760,4022"/>
            <TextEquiv>
               <Unicode>כבר עבר על מה שנ' שמה פביאן כמו שבארנו.</Unicode>
            </TextEquiv>
         </TextLine>
