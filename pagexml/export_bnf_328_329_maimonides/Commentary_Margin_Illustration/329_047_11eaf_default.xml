            <Coords points="985,4301 985,4252 1773,4263 1773,4301 1773,4372 985,4372"/>
            <Baseline points="987,4306 1778,4303"/>
            <TextEquiv>
               <Unicode>להקדים גם כן למסכתא הזאת</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_47e21fea">
            <Coords points="990,4388 985,4356 1746,4350 1751,4388 1746,4476 985,4471"/>
            <Baseline points="991,4393 1756,4391"/>
            <TextEquiv>
               <Unicode>הקדמות כמו שעשיתי לזבחים</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_a904f6a5">
            <Coords points="990,4481 990,4432 1762,4443 1762,4481 1757,4569 985,4547"/>
            <Baseline points="995,4484 1764,4482"/>
            <TextEquiv>
               <Unicode>ואומר שהמנחות מתחלקות חלק</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_b9bf03a0">
            <Coords points="1001,4569 1001,4525 1757,4531 1762,4575 1746,4646 990,4629"/>
            <Baseline points="1002,4572 1767,4577"/>
            <TextEquiv>
               <Unicode>ראשון לשני חלקים. יש</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_2b2b822e">
            <Coords points="990,4657 990,4613 1757,4618 1762,4662 1751,4728 985,4739"/>
            <Baseline points="991,4659 1764,4664"/>
            <TextEquiv>
               <Unicode>מנחה שנשרפת עם הקרבן והא</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_e8d27627">
            <Coords points="990,4750 979,4706 2244,4689 2255,4744 2244,4826 985,4821"/>
            <Baseline points="995,4754 2256,4748"/>
            <TextEquiv>
               <Unicode>באה עמו. וזו היא נקראת מנחת נסכים ואי איפשר</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_e00eb828">
            <Coords points="990,4837 990,4804 2250,4804 2250,4837 2250,4908 990,4914"/>
            <Baseline points="995,4838 2253,4843"/>
            <TextEquiv>
               <Unicode>לאותו קרבן להיות מבלעדה ויש מנחה שהיא מנחה</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_8d8c6817">
            <Coords points="985,4930 985,4892 2255,4892 2255,4930 2255,5018 985,5018"/>
            <Baseline points="987,4933 2260,4931"/>
            <TextEquiv>
               <Unicode>בפני עצמה ר"ל שאינה מנחת נסכים היא כוללת צמור</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_61e05855">
            <Coords points="979,5018 969,4974 2244,4969 2255,5012 2250,5073 979,5078"/>
            <Baseline points="984,5021 2260,5015"/>
            <TextEquiv>
               <Unicode>ויחיד. ואנחנו כבר באתנו בהקדמות שלמסכת זבחים</Unicode>
            </TextEquiv>
         </TextLine>
