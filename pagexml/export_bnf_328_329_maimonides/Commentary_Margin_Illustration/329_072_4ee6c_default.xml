            <Baseline points="2114,2232 2846,2230"/>
            <TextEquiv>
               <Unicode>ר' מאיר אומ' אינו מותר להן</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_4deb2c01">
            <Coords points="2106,2308 2101,2264 3354,2253 3365,2324 3349,2379 2095,2363"/>
            <Baseline points="2107,2313 3368,2325"/>
            <TextEquiv>
               <Unicode>לצאו לשות כדלי וקמח קותם הקרכת השמר שמא</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_79de50ea">
            <Coords points="2106,2390 2106,2352 3343,2368 3349,2406 3332,2488 2095,2450"/>
            <Baseline points="2110,2393 3354,2409"/>
            <TextEquiv>
               <Unicode>ישלחו יד ויאכלו ממנו קודם הקרבה. ולפיכך אמר</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_d5331d6c">
            <Coords points="2106,2494 2106,2461 3354,2461 3354,2494 3349,2581 2101,2559"/>
            <Baseline points="2110,2499 3357,2496"/>
            <TextEquiv>
               <Unicode>שלא כרצון חנמים. ור' יהודה אינו חושש לז</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_5f53e41e">
            <Coords points="2106,2581 2106,2549 3349,2549 3349,2581 3343,2669 2101,2652"/>
            <Baseline points="2110,2586 3350,2584"/>
            <TextEquiv>
               <Unicode>ולפיכך אמ' כרצון חכמי' והלכה בר' יהודה. ונאת'</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_0a422afb">
            <Coords points="2095,2669 2090,2631 3338,2625 3349,2674 3332,2740 2084,2729"/>
            <Baseline points="2100,2674 3354,2679"/>
            <TextEquiv>
               <Unicode>באיסור אכילת החדש שני כתובים ואמ' ער עצם</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_2f93cb1e">
            <Coords points="2084,2756 2084,2713 3343,2718 3349,2762 3332,2822 2073,2811"/>
            <Baseline points="2085,2761 3350,2766"/>
            <TextEquiv>
               <Unicode>היום הזה. ונאמ' עד הביאכם אם קרבן אלהיכם</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_2463ebf0">
            <Coords points="2090,2844 2084,2800 3338,2795 3349,2849 3338,2910 2084,2920"/>
            <Baseline points="2092,2845 3350,2850"/>
            <TextEquiv>
               <Unicode>כשהיה בית המקרש קיים לא נאסר אלא קודם ה-</Unicode>
            </TextEquiv>
         </TextLine>
