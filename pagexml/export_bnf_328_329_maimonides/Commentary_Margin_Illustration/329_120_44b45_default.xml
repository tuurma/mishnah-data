         <Coords points="1775,759 1775,2524 935,2539 858,2480 482,2422 453,832 526,759 730,745"/>
         <TextLine id="eSc_line_d65e2011">
            <Coords points="460,805 455,756 1759,750 1770,811 1759,876 449,876"/>
            <Baseline points="466,808 1773,813"/>
            <TextEquiv>
               <Unicode>בולטת ורוב כני אדם מקום טיבודם דומה לגומא ב-</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_c9451116">
            <Coords points="471,876 471,832 1749,843 1754,887 1738,942 460,931"/>
            <Baseline points="477,877 1758,890"/>
            <TextEquiv>
               <Unicode>שנוח הבטן. ויש מהן שטיבורם בולט על שטח הגוף</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_86e79709">
            <Coords points="482,948 482,909 1749,909 1754,964 1738,1030 471,1002"/>
            <Baseline points="484,950 1758,966"/>
            <TextEquiv>
               <Unicode>דומה לכולת וזהו הנקרא טיבורם יוצא והוא מום. ׆'</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_1e2e57a3">
            <Coords points="482,1041 482,997 1754,1002 1759,1046 1749,1112 477,1117"/>
            <Baseline points="484,1042 1762,1047"/>
            <TextEquiv>
               <Unicode>ונכפה ידוע. ונדא כן לפי שמתהפך שלא ברצונ</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_ade665ac">
            <Coords points="477,1117 477,1079 1754,1085 1759,1134 1743,1194 466,1178"/>
            <Baseline points="480,1122 1762,1138"/>
            <TextEquiv>
               <Unicode>בשעה שהוא כורע ורוח קצרית מקרים מחמת מרה</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_f3ef1976">
            <Coords points="471,1205 471,1167 1754,1172 1759,1222 1743,1287 460,1271"/>
            <Baseline points="477,1210 1762,1226"/>
            <TextEquiv>
               <Unicode>השחורה כגון הנקראין בלשון ערב ממלבנין וממדודין</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_774f452d">
            <Coords points="477,1304 471,1276 1749,1271 1754,1304 1754,1375 477,1380"/>
            <Baseline points="480,1308 1758,1306"/>
            <TextEquiv>
               <Unicode>שגברה עליהן המרה השחורה כל כך עד שנפסדו ׆'</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_82c95113">
            <Coords points="477,1391 477,1353 1749,1364 1754,1402 1738,1457 466,1446"/>
            <Baseline points="480,1392 1758,1405"/>
            <TextEquiv>
               <Unicode>פעולות הגוף וכמותיו. ומעושכן הוא ש' מי שכים</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_b52a583b">
            <Coords points="466,1485 466,1435 866,1435 871,1430 910,1391 910,1386 910,1386 915,1386 915,1386 915,1386 921,1386 921,1386 926,1386 1156,1391 1156,1391 1156,1391 1162,1391 1162,1391 1162,1391 1167,1391 1167,1391 1167,1397 1195,1424 1217,1441 1749,1419 1759,1496 1743,1567 449,1545"/>
            <Baseline points="469,1487 1762,1500"/>
            <TextEquiv>
               <Unicode>הביצים שלן ארוך ער שמוויע' ללרסובתן. ואין העש</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_b5da2ae7">
            <Coords points="477,1578 477,1534 1749,1539 1754,1594 1738,1660 466,1633"/>
            <Baseline points="480,1579 1755,1595"/>
            <TextEquiv>
               <Unicode>בין שיהיה מלא רוח או לחה להיות מרוח אשך. או</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_ce8c3583">
            <Coords points="482,1671 482,1633 1749,1638 1754,1676 1738,1742 471,1731"/>
            <Baseline points="484,1674 1755,1679"/>
            <TextEquiv>
               <Unicode>שיהיה הכים ארוך מדולדל ואין בו רוח ולא מורסא</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_b59580e8">
            <Coords points="477,1764 471,1731 1743,1726 1749,1764 1743,1852 471,1841"/>
            <Baseline points="480,1765 1751,1770"/>
            <TextEquiv>
               <Unicode>הכל מום. ובעל גבר על משקל ארץ ולפעמי' קורא</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_290cf5b7">
            <Coords points="487,1852 487,1813 1749,1819 1754,1857 1738,1934 477,1918"/>
            <Baseline points="488,1856 1755,1861"/>
            <TextEquiv>
               <Unicode>גובר על משקל גובל. והוא מי שהאבר שלו ארוך</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_58272d99">
            <Coords points="487,1945 487,1901 1743,1907 1749,1950 1732,2022 477,2016"/>
            <Baseline points="488,1948 1751,1953"/>
            <TextEquiv>
               <Unicode>טד שמגיע לארכובתו. וזה חולי מתחלואי האב</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_b5323222">
            <Coords points="482,2022 482,1983 1759,1994 1759,2033 1743,2104 471,2076"/>
            <Baseline points="484,2024 1762,2037"/>
            <TextEquiv>
               <Unicode>וכבר הזהידו עליו הרופאים וזכרו איתו. ומה שאמר</Unicode>
            </TextEquiv>
