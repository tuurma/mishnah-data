            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_010b55b3">
            <Coords points="971,3689 971,3650 2240,3661 2245,3700 2229,3782 960,3755"/>
            <Baseline points="976,3690 2249,3702"/>
