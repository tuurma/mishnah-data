         <Coords points="3351,2360 3336,4995 2126,5010 2024,4951 2038,2506 2068,2316 2141,2272"/>
         <TextLine id="eSc_line_b05b969e">
            <Coords points="2073,2330 2068,2275 2315,2264 2321,2330 2321,2396 2073,2413"/>
            <Baseline points="2077,2331 2326,2336"/>
            <TextEquiv>
               <Unicode>סוריא</Unicode>
            </TextEquiv>
