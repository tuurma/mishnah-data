         <TextLine id="eSc_line_85a052c7">
            <Coords points="2068,2418 2068,2369 3338,2385 3344,2435 3327,2506 2057,2484"/>
            <Baseline points="2070,2423 2581,2416 3345,2439"/>
            <TextEquiv>
               <Unicode>היא הארץ שכיבש דור חויץ מארץ יש' והיא מא</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_49ec0a45">
            <Coords points="2062,2506 2062,2468 3327,2484 3333,2523 3316,2589 2051,2583"/>
            <Baseline points="2063,2511 3334,2527"/>
            <TextEquiv>
               <Unicode>דמשק וחלב. וכבר התבאר בפרק שושי ממפכת</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_92d14bcd">
            <Coords points="2062,2594 2062,2556 3316,2556 3322,2605 3305,2671 2051,2660"/>
            <Baseline points="2066,2595 3327,2608"/>
            <TextEquiv>
               <Unicode>דמאי שהיא אינה כארץ ישראל בכל דיניה ולא</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_082fd1b2">
            <Coords points="2062,2671 2062,2616 3327,2638 3327,2693 3311,2754 2051,2748"/>
            <Baseline points="2063,2676 3330,2696"/>
            <TextEquiv>
               <Unicode>כמו שהוא חוצה לארין. וביארעו שם סיבת זה וב</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_d0ca6827">
            <Coords points="2062,2765 2062,2726 3333,2743 3333,2787 3316,2853 2051,2842"/>
            <Baseline points="2063,2768 3334,2791"/>
            <TextEquiv>
               <Unicode>תוספת כליה בשלשה שוה סוריא לארץ ישראל וב-</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_a23a580b">
            <Coords points="2057,2853 2057,2809 3322,2820 3327,2869 3311,2924 2046,2908"/>
            <Baseline points="2059,2856 3330,2872"/>
            <TextEquiv>
               <Unicode>שלשה לחוצה לארץ עפרה טמא בחוצה לארץ</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_9b04a491">
            <Coords points="2068,2935 2068,2897 3322,2908 3327,2952 3311,3023 2057,2990"/>
            <Baseline points="2070,2936 3330,2956"/>
            <TextEquiv>
               <Unicode>המביא גני מסורייא כמביא מחוצה לארץ המוכר'</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_df5d062c">
            <Coords points="2062,3018 2062,2974 3316,2985 3322,3040 3305,3100 2051,3084"/>
            <Baseline points="2063,3020 3327,3044"/>
            <TextEquiv>
               <Unicode>עברו לסוריא כמוכר מחוצה לארץ ושלשה לארץ</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_46654996">
            <Coords points="2062,3111 2062,3073 3316,3084 3322,3122 3305,3183 2051,3172"/>
            <Baseline points="2066,3112 3327,3128"/>
            <TextEquiv>
               <Unicode>ישראל. הקונה בסורייא כקינה בכוור שבירושלם</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_5b275e73">
            <Coords points="2057,3194 2057,3155 3333,3172 3333,3210 3316,3282 2046,3260"/>
            <Baseline points="2059,3196 3338,3216"/>
            <TextEquiv>
               <Unicode>ימיבת במעשרית ובשביעית. ואם יכול להכנס לה</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_ff591862">
            <Coords points="2051,3276 2051,3221 3316,3249 3322,3293 3305,3364 2040,3347"/>
            <Baseline points="2055,3281 3327,3297"/>
            <TextEquiv>
               <Unicode>בטהדה ניפורה. וכבר הוכפל פעמ' בזרעים שאשר</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_714bfa6b">
            <Coords points="2051,3369 2051,3320 3322,3336 3327,3386 3311,3463 2040,3452"/>
            <Baseline points="2052,3372 3330,3388"/>
            <TextEquiv>
               <Unicode>יהיה לו קרקע לבד בסורייא הוא. אשר ימייבהו ש</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_0aa7c4a4">
            <Coords points="2051,3457 2051,3419 3316,3430 3322,3468 3305,3545 2040,3523"/>
            <Baseline points="2052,3460 3323,3473"/>
            <TextEquiv>
               <Unicode>יוציא ממנו. ר"ל מדה שיצמת בזה הקרקע המעש'</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_d73ddc07">
            <Coords points="2040,3556 2040,3523 3322,3523 3322,3556 3322,3622 2040,3644"/>
            <Baseline points="2044,3559 3327,3557"/>
            <TextEquiv>
               <Unicode>והוא עניין אמרם כקונה בכדיר שבירושלם כמוש</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_3b6434d7">
            <Coords points="2062,3639 2062,3606 3322,3606 3322,3639 3316,3716 2057,3705"/>
            <Baseline points="2063,3644 3323,3641"/>
            <TextEquiv>
               <Unicode>ביארט בסוף חלה. ועניין להכנס לה בטהרה ש-</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_afc62fcf">
            <Coords points="2051,3727 2051,3661 3316,3688 3322,3732 3305,3809 2040,3787"/>
            <Baseline points="2055,3728 3327,3733"/>
            <TextEquiv>
               <Unicode>תפיה דביקה לארץ ישראל גבאל בגבול לא הכדל</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_dba1893f">
            <Coords points="2062,3820 2062,3760 3316,3782 3322,3826 3311,3886 2057,3892"/>
            <Baseline points="2066,3823 3327,3828"/>
            <TextEquiv>
               <Unicode>ביניהם ארץ אחרת ילא מקום טמא. ובתוספתא</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_dc659ee7">
            <Coords points="2051,3908 2046,3864 3311,3848 3322,3914 3305,3985 2040,3969"/>
            <Baseline points="2055,3911 3323,3916"/>
            <TextEquiv>
               <Unicode>כמיה תהא סמיכה ויהא יכול להכנס לה בטהרה</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_e33bdacf">
