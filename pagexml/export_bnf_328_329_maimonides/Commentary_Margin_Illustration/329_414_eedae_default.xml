            <Coords points="2094,2149 2083,2111 3357,2094 3368,2133 3363,2221 2094,2232"/>
            <Baseline points="2098,2154 3373,2137"/>
            <TextEquiv>
               <Unicode>מקום מקוער יתקביצו בו המים והקכויץ ממיו שקפות</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_ce38b111">
            <Coords points="2111,2232 2100,2193 3351,2182 3363,2221 3357,2315 2111,2315"/>
            <Baseline points="2116,2235 3365,2222"/>
            <TextEquiv>
               <Unicode>המים ואמ שבהר אם יראה בו חפירה וישוב</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_cbef648a">
            <Coords points="2117,2326 2106,2293 3346,2271 3357,2309 3351,2375 2117,2397"/>
            <Baseline points="2120,2327 3362,2313"/>
            <TextEquiv>
               <Unicode>טותו כזם הכלי. וזה ימצא ברוב ההרים הנה לא</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_a38ba6b9">
            <Coords points="2111,2403 2106,2353 3363,2359 3368,2397 3363,2475 2111,2475"/>
            <Baseline points="2116,2408 3369,2402"/>
            <TextEquiv>
               <Unicode>נחשבה כמו כלי אבנים אבל משפטה משפט ׆'</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_409e32a3">
            <Coords points="2122,2497 2111,2458 3351,2442 3363,2480 3357,2557 2122,2563"/>
            <Baseline points="2123,2499 3365,2486"/>
            <TextEquiv>
               <Unicode>חפירה בעצם הארץ ולזה אין ממלאין בה ואין</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_eaa9c9a5">
            <Coords points="2122,2590 2111,2552 3357,2535 3368,2579 3363,2673 2122,2673"/>
            <Baseline points="2123,2591 3373,2582"/>
            <TextEquiv>
               <Unicode>מקרשין בה ואין מזין ממנה. ואם תהיה באהל</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_13b70e91">
            <Coords points="2117,2679 2106,2640 3351,2623 3363,2662 3357,2728 2117,2745"/>
            <Baseline points="2120,2683 3365,2666"/>
            <TextEquiv>
               <Unicode>המת לא תצערך צמיד פעל אבל תהיה כמו</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_9684966a">
            <Coords points="2117,2767 2111,2668 2370,2668 2370,2668 2370,2668 2376,2668 2376,2668 2376,2668 2381,2668 2381,2668 2381,2673 2381,2673 2431,2717 3357,2695 3368,2750 3363,2833 2117,2833"/>
            <Baseline points="2120,2768 3373,2754"/>
            <TextEquiv>
               <Unicode>הבור והחות אליר בבית אשר בכסוי לבד' מולנו</Unicode>
