            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_e37fcbbf">
            <Coords points="2065,1147 2060,1109 3318,1093 3329,1131 3323,1191 2065,1230"/>
            <Baseline points="2071,1150 3334,1133"/>
            <TextEquiv>
