         <TextLine id="eSc_line_ef855ff8">
            <Coords points="2071,1054 2060,1010 3318,988 3329,1043 3318,1125 2065,1114"/>
            <Baseline points="2075,1055 3334,1045"/>
            <TextEquiv>
