            <Baseline points="2125,3582 2297,3579"/>
            <TextEquiv>
               <Unicode>כונת</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_33031960">
            <Coords points="2119,3672 2108,3634 2793,3628 2887,3584 2887,3584 2892,3584 2892,3584 2898,3584 3157,3584 3157,3584 3157,3584 3163,3584 3163,3584 3163,3584 3168,3584 3168,3584 3168,3590 3202,3617 3306,3584 3312,3584 3312,3584 3312,3584 3318,3584 3318,3584 3323,3584 3323,3584 3323,3584 3329,3584 3389,3623 3384,3661 3384,3722 2119,3733"/>
            <Baseline points="2121,3677 3385,3664"/>
            <TextEquiv>
               <Unicode>אמרו תחלה לעולם שאם נטמא ואחר נקכא אחר</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_17222d43">
            <Coords points="2119,3755 2108,3717 3362,3706 3373,3750 3367,3827 2119,3832"/>
            <Baseline points="2121,3758 3378,3752"/>
            <TextEquiv>
               <Unicode>זה ואחר נמחה ואחר נקפא ואחר נמחה. אפילו</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_2afd1f58">
            <Coords points="2119,3849 2108,3810 3362,3794 3373,3838 3367,3910 2119,3926"/>
            <Baseline points="2125,3854 3378,3840"/>
            <TextEquiv>
               <Unicode>היה זה אלף העמ' הנה הוא תחלה בכל עניין מהן בין</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_4363f288">
            <Coords points="2119,3937 2114,3821 2567,3821 2567,3821 2567,3821 2572,3821 2572,3821 2572,3821 2578,3821 2578,3821 2578,3827 2600,3843 2649,3893 3367,3888 3378,3926 3378,3981 2119,4003"/>
            <Baseline points="2125,3942 3382,3928"/>
            <TextEquiv>
               <Unicode>הקפויה או טוה סין שהיה החלה לל עורלן פין או</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_566a68fd">
            <Coords points="2119,4020 2108,3976 3362,3959 3373,4003 3367,4081 2119,4097"/>
            <Baseline points="2121,4023 3374,4009"/>
            <TextEquiv>
               <Unicode>יקפאו באמת ומשקה ייחשב ואפי' בעת ההקפאה</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_f2db77f7">
            <Coords points="2119,4114 2108,4064 3362,4042 3373,4097 3367,4169 2119,4185"/>
            <Baseline points="2121,4115 3378,4101"/>
            <TextEquiv>
               <Unicode>ופי' גוף הגף המדובק כמו וגוש עפר. והכוונה</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_dbab0adc">
            <Coords points="2119,4213 2108,4174 3362,4130 3373,4185 3367,4252 2119,4279"/>
            <Baseline points="2121,4214 3378,4190"/>
            <TextEquiv>
               <Unicode>פה שיהיו כלל הזתים שמאין מקובצין מחובדי גשם</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_cf4dfb75">
            <Coords points="2119,4296 2108,4252 3362,4235 3373,4279 3367,4345 2119,4367"/>
            <Baseline points="2121,4298 3378,4281"/>
            <TextEquiv>
               <Unicode>אחד. וכבר פרשנו ובארנו בפתיחה שאוכלי טמא'</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_7be7a1d7">
            <Coords points="2119,4378 2108,4340 3367,4323 3378,4367 3373,4428 2119,4439"/>
            <Baseline points="2121,4383 3382,4370"/>
            <TextEquiv>
               <Unicode>לא יטמאו הכלים ושמשקין טמאין יטמאן הכלים</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_16565673">
            <Coords points="2131,4467 2119,4428 3367,4412 3378,4456 3373,4522 2131,4544"/>
            <Baseline points="2132,4471 3382,4458"/>
            <TextEquiv>
               <Unicode>והתבאר גם כן בכלים שמשקין טמאין מטמאין</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_ae05aaeb">
            <Coords points="2131,4566 2119,4527 3367,4494 3378,4549 3373,4632 2131,4649"/>
            <Baseline points="2132,4567 3382,4554"/>
            <TextEquiv>
               <Unicode>התנור באויר אם היו יותר מכביצה בעת מה ש-</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_90f50f5f">
            <Coords points="2131,4654 2119,4610 3362,4588 3373,4632 3367,4698 2131,4720"/>
            <Baseline points="2132,4655 3374,4634"/>
            <TextEquiv>
               <Unicode>וצא השמן בחום האש יטמא בכביצה הנשארת</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_e368d031">
            <Coords points="2119,4731 2108,4687 3367,4671 3378,4715 3373,4792 2119,4798"/>
            <Baseline points="2125,4732 3382,4719"/>
            <TextEquiv>
               <Unicode>משאר הזתים קודם שריפתן וישובו משקי' טמאין</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_e6e766bc">
            <Coords points="2125,4809 2114,4770 3367,4759 3384,4803 3378,4875 2125,4880"/>
            <Baseline points="2128,4813 3385,4807"/>
            <TextEquiv>
               <Unicode>ויטמאו התנור להגיעם באוירו. וזה אם יהין וּ׆'</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_c6455d95">
            <Coords points="2119,4908 2108,4864 3373,4858 3384,4902 3373,4991 2114,4974"/>
            <Baseline points="2121,4912 3385,4903"/>
            <TextEquiv>
               <Unicode>מקובינין. אמנם אם יהיו הזתים גרגרים מובדלין או</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_b3af999d">
            <Coords points="2119,5002 2108,4963 3367,4946 3378,4985 3373,5051 2119,5062"/>
            <Baseline points="2121,5004 3382,4991"/>
            <TextEquiv>
               <Unicode>חלקים לא יהיה ככל כביצה ואפי' היה איזה שיעוו</Unicode>
            </TextEquiv>
         </TextLine>
      </TextRegion>
      <TextRegion id="eSc_textblock_89119d44" custom="structure {type:Commentary;}">
