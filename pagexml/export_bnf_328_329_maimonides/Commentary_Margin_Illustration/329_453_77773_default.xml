      <TextRegion id="eSc_textblock_ad04d0e7" custom="structure {type:Commentary;}">
         <Coords points="2311,2063 2355,5066 2252,5111 1105,5096 1046,5066 1017,2093 1784,2034"/>
         <TextLine id="eSc_line_8d8e1338">
            <Coords points="1018,2111 1012,2061 2290,2067 2296,2105 2290,2177 1018,2188"/>
            <Baseline points="1023,2116 2301,2110"/>
            <TextEquiv>
               <Unicode>כבר התבאר במקומות רבים ממה שקדם שהכלי'</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_1df75adb">
            <Coords points="1018,2199 1007,2166 2279,2161 2290,2194 2285,2266 1018,2271"/>
            <Baseline points="1023,2204 2294,2198"/>
            <TextEquiv>
               <Unicode>יטמאו משקין וישובו אלה הכלים שני לטומאה</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_b9c6716c">
            <Coords points="1018,2293 1007,2255 2285,2249 2296,2288 2285,2365 1012,2354"/>
            <Baseline points="1023,2296 2297,2290"/>
            <TextEquiv>
               <Unicode>וזה מדרבנן. ועוד יתבאר בטו' מכלים שמשקין ׆'</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_4cf62eef">
            <Coords points="1029,2382 1018,2332 2285,2327 2296,2376 2285,2454 1023,2448"/>
            <Baseline points="1030,2385 2297,2379"/>
            <TextEquiv>
               <Unicode>טמאין שנגעןו באחורי כלים נטמאו אחוריו ולא</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_6985b1ad">
            <Coords points="1029,2476 1023,2421 2290,2426 2296,2470 2290,2531 1029,2559"/>
            <Baseline points="1030,2477 2297,2471"/>
            <TextEquiv>
               <Unicode>נטמא תוכו לתרומה. ובארנו בפתיחה זו שמשקן</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_ac628eb0">
            <Coords points="1007,2564 996,2526 2274,2503 2285,2553 2274,2642 1001,2636"/>
            <Baseline points="1012,2569 2290,2555"/>
            <TextEquiv>
               <Unicode>טמאין תחלה לשלם ואפי' נטוואו בשיני לטומאה.</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_f2101b4a">
            <Coords points="1007,2653 996,2614 2285,2608 2296,2647 2290,2714 1007,2725"/>
            <Baseline points="1008,2654 2301,2648"/>
            <TextEquiv>
               <Unicode>ואם נטמא אחורי הכלי במשקין טמאין שב אחורי</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_13b61e3d">
            <Coords points="1029,2741 1018,2702 2285,2669 2296,2730 2290,2791 1029,2819"/>
            <Baseline points="1034,2742 2297,2736"/>
            <TextEquiv>
               <Unicode>הכל שיני.. ואם נגען אוכלין טהורן באחורי זה</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_f87c6e82">
            <Coords points="1034,2824 1023,2785 2285,2758 2296,2819 2290,2879 1034,2890"/>
            <Baseline points="1038,2827 2297,2821"/>
            <TextEquiv>
               <Unicode>הכלי לא נפסלו והיה ההקש שתפסל ותשוב שלישי</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_46a200c8">
            <Coords points="1029,2918 1018,2879 2285,2868 2296,2907 2290,2973 1029,2984"/>
            <Baseline points="1030,2919 2301,2909"/>
            <TextEquiv>
               <Unicode>אך ר' אליעזר יקל בזה להיותה טומאה קלה מדרבנן.</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_41dd55ab">
            <Coords points="1034,3001 1029,2957 2296,2957 2302,2995 2290,3084 1029,3073"/>
            <Baseline points="1038,3004 2305,2998"/>
            <TextEquiv>
               <Unicode>והוא אמרם מאי שנא אחורי כלים רנקט משום ר</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_299b3a5f">
            <Coords points="1040,3078 1040,3040 2296,3045 2302,3084 2285,3145 1029,3139"/>
            <Baseline points="1045,3081 2305,3086"/>
            <TextEquiv>
               <Unicode>דקילא טומאתן. ופה אמ' מטמאין את המשקין ׆'</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_55523fe2">
            <Coords points="1040,3172 1040,3134 2296,3134 2296,3172 2296,3250 1040,3250"/>
            <Baseline points="1041,3173 2297,3178"/>
            <TextEquiv>
               <Unicode>אפי' דחולין ואין פיסלין את האורלין אפי' מתרומה.</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_305c3435">
            <Coords points="1040,3261 1029,3222 2285,3200 2296,3255 2285,3338 1034,3333"/>
            <Baseline points="1041,3262 2301,3256"/>
            <TextEquiv>
               <Unicode>ור' יהושע אומ' שאחוקי זה הכלי גם כן פופלין את</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_c9f41416">
            <Coords points="1045,3344 1040,3310 2302,3305 2307,3349 2290,3421 1034,3410"/>
            <Baseline points="1049,3346 2308,3351"/>
            <TextEquiv>
               <Unicode>האוכלין כמו כן שיני קל וחומר מיטובול יום אשר</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_34a3188b">
            <Coords points="1045,3438 1045,3404 2302,3404 2302,3438 2296,3521 1040,3515"/>
            <Baseline points="1049,3442 2305,3440"/>
            <TextEquiv>
               <Unicode>פיסל את התרומה ואינו מנמא את המשקין כמו</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_338d887e">
            <Coords points="1040,3526 1040,3487 2302,3487 2302,3526 2296,3609 1034,3592"/>
            <Baseline points="1045,3527 2305,3532"/>
            <TextEquiv>
               <Unicode>שבארנו בעימיני מפרה כל שכן זה אשר יטמר</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_127cf4d9">
            <Coords points="1051,3614 1045,3554 2296,3570 2302,3609 2296,3692 1051,3692"/>
            <Baseline points="1052,3615 2305,3613"/>
            <TextEquiv>
               <Unicode>משקין שהוא יפסול לא דקיש קל וחומר בין טבול</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_616f9c73">
            <Coords points="1051,3703 1040,3664 2290,3653 2302,3697 2290,3780 1045,3775"/>
            <Baseline points="1056,3704 2305,3702"/>
            <TextEquiv>
               <Unicode>יום דאוריתא ואחורי הכלים דרבין. לפי שהוא אומ'</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_cc4abc5a">
            <Coords points="1051,3791 1051,3753 2307,3753 2307,3791 2302,3880 1045,3869"/>
            <Baseline points="1056,3792 2308,3797"/>
            <TextEquiv>
               <Unicode>אין דען דברי סופרים מד'אוריתא. ובא שמעון אחי</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_de2b20f5">
            <Coords points="1051,3874 1051,3836 2302,3841 2307,3885 2290,3952 1040,3935"/>
            <Baseline points="1052,3877 2312,3889"/>
            <TextEquiv>
               <Unicode>עזריה ואמ' שדברי ר' יהושע אשר אמ' שאחורי</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_6175c3bb">
            <Coords points="1051,3963 1051,3924 2302,3941 2307,3979 2290,4046 1040,4024"/>
            <Baseline points="1052,3966 2312,3982"/>
            <TextEquiv>
               <Unicode>הכלי פוסלין את התרומה אינינו אמת בשום פנים</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_0f845dd5">
            <Coords points="1051,4057 1051,4018 2302,4029 2307,4068 2290,4129 1040,4117"/>
            <Baseline points="1052,4058 2312,4070"/>
            <TextEquiv>
               <Unicode>וכן מאמר ר' אליעזר אשר אמ' אין פוסלין אינים</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_5dedb24b">
            <Coords points="1051,4145 1051,4106 2307,4117 2313,4156 2296,4239 1040,4228"/>
