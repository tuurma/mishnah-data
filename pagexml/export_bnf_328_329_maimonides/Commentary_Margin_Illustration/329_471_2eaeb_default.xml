            <Baseline points="967,3329 1231,3334"/>
            <TextEquiv>
               <Unicode>בעוביה</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_26cb6985">
            <Coords points="972,3423 972,3379 1889,3385 1906,3385 2154,3357 2154,3357 2160,3357 2160,3357 2165,3357 2165,3357 2254,3390 2254,3429 2243,3495 966,3495"/>
            <Baseline points="974,3428 2255,3433"/>
            <TextEquiv>
               <Unicode>ובחללה כמו עובי גשמה ונקבה ושיעורה אמר</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_ebc775e0">
            <Coords points="966,3512 966,3473 2237,3473 2243,3517 2226,3589 955,3573"/>
            <Baseline points="971,3516 2248,3521"/>
            <TextEquiv>
               <Unicode>שיעור נקבה קוטר שתי אצבעות. ואם תהיה בין</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_9d4c355e">
            <Coords points="966,3606 955,3567 2237,3562 2248,3600 2243,3683 966,3689"/>
            <Baseline points="971,3608 2251,3606"/>
            <TextEquiv>
               <Unicode>שני מקוואות נקב שיעורו זה השיעור הנה נתערלו</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_4dc8d82e">
            <Coords points="977,3689 966,3650 2232,3644 2243,3683 2232,3771 972,3755"/>
            <Baseline points="978,3693 2248,3687"/>
            <TextEquiv>
               <Unicode>השני מקוואות ושבו מקוה אחך. ואמרו מפני ש-</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_75fc6861">
            <Coords points="972,3777 966,3711 2237,3733 2243,3771 2237,3838 972,3843"/>
            <Baseline points="974,3782 2248,3775"/>
            <TextEquiv>
               <Unicode>היא מן התורה ר"ל' חיוב הטבילה במקוה המים. ולשו'</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_a0383348">
            <Coords points="972,3865 961,3827 2243,3821 2254,3860 2248,3943 972,3954"/>
            <Baseline points="974,3870 2255,3864"/>
            <TextEquiv>
               <Unicode>התוספתא כזית מן הנבילה וכעדשה מן השרין ספק</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_7c993127">
            <Coords points="972,3959 972,3920 2248,3920 2248,3959 2243,4047 966,4025"/>
            <Baseline points="974,3962 2251,3960"/>
            <TextEquiv>
               <Unicode>יש בהן כשיעור ספקאין בהן כשיעור ספיקן טמאל</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_2eb04c69">
            <Coords points="972,4047 972,4009 2248,4014 2254,4053 2237,4125 961,4108"/>
            <Baseline points="974,4050 2255,4055"/>
            <TextEquiv>
               <Unicode>שכל דבר שעקרו מן התורה ושיעורו מדברי סופרים</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_0ace5cd7">
            <Coords points="972,4136 961,4097 2243,4092 2254,4130 2243,4213 966,4202"/>
            <Baseline points="974,4139 2255,4136"/>
            <TextEquiv>
               <Unicode>ספיקו טמא. וכבר בארנו שזה לא יחסר מאמרם</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_8fb9e045">
            <Coords points="972,4224 972,4186 2243,4191 2248,4230 2237,4296 966,4302"/>
            <Baseline points="974,4227 2251,4232"/>
            <TextEquiv>
               <Unicode>שיעורן הלכה למשה מסיני. לפי שכל מה שאינו</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_b077493b">
            <Coords points="966,4313 961,4268 2248,4268 2254,4307 2248,4373 966,4384"/>
            <Baseline points="971,4315 2255,4313"/>
            <TextEquiv>
               <Unicode>בפסוק יקראוהו מדברי סופרי' ומבדית המים כמן</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_b1b8581c">
            <Coords points="977,4401 977,4346 2248,4357 2254,4406 2243,4473 972,4473"/>
            <Baseline points="978,4404 2255,4409"/>
            <TextEquiv>
               <Unicode>עדשי המים והבעל חיים המתיילדין במים. אמ'</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_54e6fd93">
            <Coords points="966,4495 955,4451 2243,4445 2254,4489 2248,4556 966,4561"/>
            <Baseline points="971,4496 2259,4490"/>
            <TextEquiv>
               <Unicode>שאם יהיו בזה המקום אשר הוא כשפופרת הנוד</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_bb5db8ab">
            <Coords points="1342,4583 1331,4533 2248,4517 2259,4578 2254,4644 1342,4649"/>
            <Baseline points="1346,4588 2262,4582"/>
            <TextEquiv>
               <Unicode>לא יצר אותו ואין הלכה כרשב'ג':</Unicode>
