      <TextRegion id="eSc_textblock_d8c86cfd" custom="structure {type:Commentary;}">
         <Coords points="1673,483 1693,678 1654,708 1271,717 710,698 661,668 661,463"/>
         <TextLine id="eSc_line_6251cbb9">
            <Coords points="648,499 648,481 703,473 1455,477 1518,492 1515,521 1452,529 648,540"/>
            <Baseline points="649,502 1520,493"/>
            <TextEquiv>
               <Unicode>חוג הוא הסומאק אשר בארץ הצבי והוא צמח שיעבדו בו</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_8469563e">
            <Coords points="655,555 666,536 907,540 970,525 1152,536 1196,521 1326,532 1433,518 1655,518 1681,540 1678,569 1630,581 1433,569 1411,584 655,584"/>
            <Baseline points="659,557 1683,544"/>
            <TextEquiv>
               <Unicode>העורות ויש לו אשכלות כאשכלות הגפן ויאכלו. חרובין ידוע.</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_f11bb8dc">
            <Coords points="648,610 652,588 1144,592 1189,577 1670,577 1685,603 1670,640 648,636"/>
            <Baseline points="651,614 1686,604"/>
            <TextEquiv>
               <Unicode>ואגוזי' ושקרי' כמו כן ידועי'. ואלה האילנו' כלם נמצאי' בהם התנאי'</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_116791ce">
            <Coords points="692,669 692,651 718,647 1129,647 1185,632 1378,643 1607,629 1655,632 1685,655 1678,688 1592,684 1544,703 1448,680 1411,706 1159,695 1044,710 981,695 837,714 692,695"/>
            <Baseline points="696,670 1686,658"/>
            <TextEquiv>
               <Unicode>הנזכדים בכל מה שיתחייב פאה ולפיכך הם מחויביים בפאה.</Unicode>
            </TextEquiv>
         </TextLine>
      </TextRegion>
      <TextRegion id="eSc_textblock_5a786855" custom="structure {type:Commentary;}">
         <Coords points="1693,1233 1693,2800 1103,2879 661,2800 661,1194"/>
         <TextLine id="eSc_line_a99067d0">
            <Coords points="648,1217 666,1195 896,1202 900,1217 896,1243 826,1254 648,1247"/>
            <Baseline points="651,1218 903,1218"/>
            <TextEquiv>
               <Unicode>דע שהפאה לא</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_9f836f92">
            <Coords points="648,1269 648,1247 759,1250 774,1236 792,1250 863,1236 896,1250 1015,1250 1029,1236 1096,1250 1166,1232 1555,1232 1689,1243 1692,1261 1689,1291 1663,1302 1241,1291 1152,1306 648,1306"/>
            <Baseline points="649,1272 1693,1262"/>
            <TextEquiv>
               <Unicode>תתחייב להוציא ממנה מעשרות. ושמור זה העקר שכל מי שלח הניח</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_44e53870">
            <Coords points="648,1328 666,1302 1148,1310 1166,1295 1574,1306 1689,1291 1692,1321 1689,1350 1659,1358 648,1365"/>
            <Baseline points="651,1329 1693,1324"/>
            <TextEquiv>
               <Unicode>פאה וקצר כל השדה כלו יוציא הפאה ממה שקצר. וכן אם לא הוציא</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_b774662c">
            <Coords points="648,1380 666,1358 896,1354 1689,1354 1692,1376 1689,1406 1655,1417 648,1410"/>
            <Baseline points="649,1383 1693,1378"/>
            <TextEquiv>
               <Unicode>מן השר לים הקצורים יוציא מן החטה אחר שידושו אותה וימרחוה</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_aebce33b">
            <Coords points="648,1439 648,1421 689,1413 1144,1421 1259,1406 1678,1406 1692,1432 1674,1469 648,1465"/>
            <Baseline points="651,1440 1693,1433"/>
            <TextEquiv>
               <Unicode>ויבדרוה. ואפי' טחנה יוציא הפאה מן הקמח. וזה העקר מבואר</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_f9dc5350">
            <Coords points="648,1491 648,1476 940,1465 1667,1465 1692,1487 1674,1524 648,1521"/>
            <Baseline points="649,1494 1693,1489"/>
            <TextEquiv>
               <Unicode>בגמר מכות. וממה שיש לך לזכור תמיד שההפקר והוא הדבר הכ</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_c23dcb6e">
            <Coords points="648,1550 648,1532 696,1521 1163,1532 1637,1517 1689,1524 1692,1543 1689,1572 1663,1580 1259,1569 1166,1583 648,1583"/>
            <Baseline points="649,1551 1693,1544"/>
            <TextEquiv>
               <Unicode>הנעזב והמשולח אינו חייב במעשרות. ופי' מריחה מריקה. וזאת</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_22787846">
            <Coords points="648,1602 648,1587 726,1587 740,1572 800,1587 1641,1572 1689,1576 1692,1598 1689,1624 1659,1635 648,1628"/>
            <Baseline points="651,1605 1693,1600"/>
            <TextEquiv>
               <Unicode>המילה מושאלת בדרך המליצה בענין ברירת החטה מן התבן וה</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_db4a9f2a">
            <Coords points="648,1661 648,1635 752,1643 766,1628 807,1643 848,1628 1126,1643 1141,1628 1244,1639 1270,1624 1348,1639 1622,1624 1674,1628 1692,1654 1689,1680 1652,1691 648,1687"/>
            <Baseline points="651,1662 1693,1657"/>
            <TextEquiv>
               <Unicode>והשלמת צרכי הזדע עד שלא ישאר לעשות בו אלא להוליכו אל האוצר</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_47f9553f">
            <Coords points="655,1717 674,1691 733,1687 1166,1698 1215,1683 1685,1683 1692,1709 1685,1750 681,1743 655,1757"/>
            <Baseline points="656,1719 1695,1711"/>
            <TextEquiv>
               <Unicode>והוא דומה כמו הממרק לכלים שאין עושין אותו המרוק אלא כש</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_dbd2d2ad">
            <Coords points="652,1772 652,1754 681,1743 1163,1754 1600,1739 1689,1746 1692,1765 1689,1794 1652,1802 652,1809"/>
            <Baseline points="654,1773 1693,1768"/>
            <TextEquiv>
               <Unicode>כשנשלם הכלי וכשיקצר הזרע הנזרע כלו ויודש ונברר מן התבן או</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_be48a625">
            <Coords points="652,1828 652,1809 670,1806 1141,1809 1263,1794 1674,1798 1692,1820 1689,1846 1667,1857 1370,1846 652,1861"/>
            <Baseline points="654,1830 1693,1823"/>
            <TextEquiv>
               <Unicode>יפריש מן הזרע ההוא תרומה ומעשרות המחוייבות לו ואחר כן יו</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_c6387837">
            <Coords points="652,1883 652,1865 722,1857 1155,1865 1203,1850 1644,1850 1689,1857 1692,1876 1689,1905 1659,1913 652,1913"/>
            <Baseline points="654,1884 1695,1879"/>
            <TextEquiv>
               <Unicode>יוציא הפאה שנתחייב בה בזרע ויתננה לעניים אבל קודם הברירה</Unicode>
