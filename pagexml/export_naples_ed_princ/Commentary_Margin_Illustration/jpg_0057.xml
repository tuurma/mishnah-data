            <Coords points="585,636 585,617 783,617 809,599 857,599 1004,614 1019,599 1037,614 1096,599 1140,617 1184,617 1203,599 1288,617 1622,610 1630,636 1626,665 1527,672 585,672"/>
            <Baseline points="588,639 1631,637"/>
            <TextEquiv>
               <Unicode>האריגות שיעשו מן הקנים והעצים כגון עריסות להשכיב עליהם ש</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_7330b276">
            <Coords points="585,695 607,665 861,665 1619,665 1622,695 1619,720 1564,728 585,728"/>
            <Baseline points="588,696 1626,696"/>
            <TextEquiv>
               <Unicode>שריגי הערים וענפיו כשיאריכו ואמר שהוא כשידלה ענפי הגפן על</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_d5243945">
            <Coords points="585,746 585,731 905,720 1611,720 1626,746 1611,786 585,775"/>
            <Baseline points="586,749 1629,750"/>
            <TextEquiv>
               <Unicode>קצת. הערים העשוי לו ונשאר קצתו אסור לו שיזרע תחת המקום</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_1ccad5ce">
            <Coords points="588,805 599,783 728,775 1534,775 1619,783 1622,801 1619,831 1575,842 805,831 588,842"/>
            <Baseline points="591,806 1624,803"/>
            <TextEquiv>
               <Unicode>הריקן מן הערים העשוי אעפ"י שאין עליו ענפים. וכל הזורע שם לא</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_ddb79b6e">
            <Coords points="585,856 585,842 621,838 879,834 1619,834 1622,856 1611,897 853,886 585,897"/>
            <Baseline points="588,860 1624,860"/>
            <TextEquiv>
               <Unicode>קדש'. ואם האריכו הענפים הנטוים על העריס עד יסבכו על הזרע</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_be40d586">
            <Coords points="588,915 588,897 618,889 706,886 1541,886 1615,893 1619,912 1615,941 1541,948 588,948"/>
            <Baseline points="591,919 1621,914"/>
            <TextEquiv>
               <Unicode>יהיה אסור אותו הזרע. וזה הדין בעצמו יתחייב בכל מי שידלה ענפי</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_04c88a14">
            <Coords points="585,970 585,945 776,941 1597,941 1626,967 1622,996 1552,1007 669,996 585,1011"/>
            <Baseline points="588,973 1629,970"/>
            <TextEquiv>
               <Unicode>הגפן על קצת ענפי אילן סרק. ואילן סרק הוא אשר לא יעשה פרי כמו</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_10156218">
            <Coords points="599,1026 599,1011 673,1000 1527,1000 1615,1007 1622,1026 1619,1081 1564,1081 1538,1062 1442,1081 1391,1059 1324,1073 1173,1059 1151,1081 1056,1081 1030,1059 997,1081 916,1077 897,1059 765,1059 743,1081 710,1081 655,1059 599,1081"/>
