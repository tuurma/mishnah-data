            <Coords points="18,930 29,912 432,904 543,919 1057,912 1061,938 1057,964 923,975 554,975 532,960 33,971 18,956"/>
            <Baseline points="22,932 1065,942"/>
            <TextEquiv>
               <Unicode>כל שהוא אוכל יצא הכופר. והוא בערבי אלחנא. והאסוס והוא</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_a81493b7">
            <Coords points="37,986 37,968 275,975 320,960 554,979 1027,964 1065,994 1046,1027 972,1031 908,1016 554,1031 383,1012 353,1027 126,1027 37,1009"/>
            <Baseline points="39,987 1068,997"/>
            <TextEquiv>
               <Unicode>בלשון עדבי נילג. והקוצה והוא בלשון ערבי אלעצפור ודומיהן.</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_d611ca29">
            <Coords points="18,1046 33,1020 167,1035 201,1020 275,1035 350,1035 368,1020 383,1035 629,1035 648,1020 689,1035 819,1035 908,1020 949,1035 1057,1031 1065,1046 1061,1083 1013,1090 18,1090"/>
            <Baseline points="22,1049 1068,1049"/>
            <TextEquiv>
               <Unicode>ונשמר יצא המופקר. וגדוליו מן הארץ יצאו כמהין ופטריות וכל זה</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_2778cfc4">
            <Coords points="26,1105 26,1090 59,1079 968,1079 1054,1090 1065,1105 1057,1146 26,1146"/>
            <Baseline points="27,1106 1068,1106"/>
            <TextEquiv>
               <Unicode>נסמך למה שאמר השם יתברך עשר תעשר ואחר כך אמר ואכלת לפ</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_7f1321af">
            <Coords points="22,1161 22,1146 197,1135 938,1135 1057,1146 1061,1161 1054,1191 905,1187 890,1202 759,1187 733,1202 651,1202 636,1187 554,1187 536,1202 417,1202 402,1187 379,1202 48,1202 22,1187"/>
            <Baseline points="24,1163 1065,1163"/>
            <TextEquiv>
               <Unicode>לפני ה' אלדיך מעשר דגנך תירושך ויצהרך. למדנו שאין חייב במעשר</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_6a8964d4">
            <Coords points="22,1217 29,1198 320,1191 1039,1191 1065,1217 1050,1254 919,1239 867,1239 852,1254 797,1239 730,1254 666,1239 640,1254 525,1239 510,1254 435,1239 413,1254 312,1254 294,1239 260,1254 167,1254 152,1239 22,1247"/>
            <Baseline points="24,1218 1068,1218"/>
            <TextEquiv>
               <Unicode>אלא היוצא מן השדה וראוי לאכילה ונשמר למאמר ית זרעך. זרעך</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_107a493e">
            <Coords points="26,1273 26,1251 111,1258 130,1243 171,1258 204,1243 238,1258 271,1243 324,1258 413,1258 432,1243 532,1243 558,1258 804,1243 849,1258 864,1243 923,1243 1035,1251 1065,1273 1061,1299 1035,1310 26,1310"/>
            <Baseline points="27,1275 1068,1275"/>
            <TextEquiv>
               <Unicode>המיוחד לך לא של הפקר לפי שאין לו בעלים. ותחלתו אוכל וסופו</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_b3e0b1f6">
            <Coords points="22,1329 22,1314 122,1299 249,1314 353,1299 905,1314 931,1299 1031,1310 1057,1295 1061,1325 1057,1355 1027,1366 59,1355 22,1370"/>
            <Baseline points="24,1330 1065,1327"/>
            <TextEquiv>
               <Unicode>אוכל הם מיני ירקות לפי שהם ראויות לאכילה תכף צמיחתן ואחר ג</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_da4bc428">
            <Coords points="22,1385 22,1370 204,1359 938,1359 1042,1362 1068,1385 1046,1426 1027,1411 871,1411 852,1426 327,1426 286,1411 193,1426 178,1411 93,1411 44,1426 22,1411"/>
            <Baseline points="24,1387 1070,1387"/>
            <TextEquiv>
               <Unicode>גמר גדולן ואין מניחין אותן בארץ אלא כדי שיגדילו. וזהו דרך רוב</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_a792b61d">
            <Coords points="22,1440 22,1426 74,1414 908,1414 1061,1422 1065,1437 1061,1466 990,1478 22,1470"/>
            <Baseline points="24,1444 1068,1439"/>
            <TextEquiv>
               <Unicode>הירקות ולפיכך הם חייבות במעשר בשעת קטנן או גדלם לפי שהם</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_3ff5bb4e">
            <Coords points="22,1493 63,1470 1057,1470 1061,1493 1050,1526 960,1522 946,1537 838,1537 815,1522 800,1537 595,1537 581,1522 536,1537 506,1522 491,1537 465,1522 365,1537 40,1537 22,1522"/>
            <Baseline points="24,1496 1065,1496"/>
            <TextEquiv>
               <Unicode>ראויות לאכילה בין גדולות בין קטנות. ומה שאינו ראוי לאכילה</Unicode>
            </TextEquiv>
         </TextLine>
         <TextLine id="eSc_line_f545a393">
            <Coords points="22,1552 22,1537 93,1530 346,1537 413,1522 506,1522 536,1537 942,1537 987,1522 1057,1526 1061,1548 1057,1578 1001,1589 22,1582"/>
            <Baseline points="24,1555 1065,1551"/>
